{
    "info": {
        "id": "scatter-chart",
        "name": "Scatter Chart"
    },
    "config": [
        {
            "fieldLabel": "X axis",
            "fieldName": "x",
            "fieldType": "dropdown",
            "valueSet": ["$COLUMN_NAMES"]
        },
        {
            "fieldLabel": "X type",
            "fieldName": "xType",
            "fieldType": "dropdown",
            "valueSet": ["default", "ordinal", "linear", "time"]
        },
        {
            "fieldLabel": "Y axis",
            "fieldName": "y",
            "fieldType": "dropdown",
            "valueSet": ["$COLUMN_NAMES"]
        },
        {
            "fieldLabel": "Y type",
            "fieldName": "yType",
            "fieldType": "dropdown",
            "valueSet": ["default", "ordinal", "linear", "time"]
        },
        {
            "fieldLabel": "Color domain",
            "fieldName": "color",
            "fieldType": "dropdown",
            "valueSet": ["$COLUMN_NAMES"]
        },
        {
            "fieldLabel": "Size domain",
            "fieldName": "size",
            "fieldType": "dropdown",
            "valueSet": ["$COLUMN_NAMES"]
        }
    ],
    "imports": {
        "shared-libs":{
            "js" :["analytics-wso2/d3.min.js", "analytics-wso2/vega.js",  "analytics-wso2/VizGrammar.min.js",
                "analytics-wso2/wso2gadgets.js", "analytics-wso2/chart-utils.js"]
        }
    }
}

