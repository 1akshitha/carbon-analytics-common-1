<serviceGroup>
    <service name="MessageConsole" scope="transportsession">
        <transports>
            <transport>https</transport>
        </transports>
        <parameter name="ServiceClass">org.wso2.carbon.analytics.messageconsole.MessageConsoleService</parameter>
    </service>

    <parameter name="adminService" locked="true">true</parameter>
    <parameter name="hiddenService" locked="true">true</parameter>
    <parameter name="AuthorizationAction" locked="true">/permission/admin/manage/analytics/messageconsole</parameter>
</serviceGroup>